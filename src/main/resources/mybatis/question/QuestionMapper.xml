    <update id="updatejhjl">
        update WTGL_JHJL
        set EJHFPPYJ=#{ejhfppyj}
        where xh = #{xh}
    </update>
    <update id="closedLoop">
        update wtgl_zrdb
        set ZRRZT='确认已完成'
        where XH = #{xh}
    </update>
    <update id="activate">
        update wtgl_zrdb
        set ZRRZT='未处理'
        where XH = #{xh}
    </update>
