    <update id="updateQuestionsStatus">
        <foreach collection="wtxxDTO" item="item" separator=";">
            update WTGL_YWCJ
            <set>
                WTZT=#{item.value},
                SFDZFK=1,
                <if test="item.WTDB !=null">
                    WTDB=WTDB+#{item.WTDB},
                </if>
                <if test="item.WDWT !=null">
                    WDWT=WDWT+#{item.WDWT},
                </if>
                <if test="item.JSSJ !=null">
                    JSSJ=#{item.JSSJ},
                </if>
            </set>
            where ID=#{item.ID}
        </foreach>
    </update>
